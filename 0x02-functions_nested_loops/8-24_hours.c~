#include "main.h"
/**
 * jack_bauer - check the code
 *
 * Return: Always 0.
 */
void jack_bauer(void){
  char hour;
  hour = '0';
 
  while (hour <= '2' + '3')
    {
      char min;
      min = '0';
      while (min <= '5' + '9')
	{
	  _putchar(hour);
	  _putchar(':');
	  _putchar(min);
	  _putchar('\n');
	  min++;
	}
      hour++;
    }
}
