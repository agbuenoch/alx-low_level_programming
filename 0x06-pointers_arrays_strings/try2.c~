#include <stdio.h>
/**
 * reverse_array - print strings in reverse
 *
 * @a: first pointer-to-integer parameter
 * @n: second parameter
*/
void trying(int n)
{
  int i = n; 
	  if(i != 0) {
	    trying(n % 10);
	    putchar((n / 10) + '0');
	  }
}

int main(void){
  trying(1994);
  putchar('\n');
  return (0);
    
}
