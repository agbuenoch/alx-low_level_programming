#include "main.h"
/**
 * print_square - check the code
 *
 * @size: first parameter
 *
 * Return: Always 0.
 */
void print_square(int size)
{
  if (size > 0)
    {
      int i;
      for (i = 0; i <= n; i++)
	{
	  int j;
	  for (j = 0; j <= n; j++)
	    {
	      _putchar('#');
	    }
	  _putchar('\n');
	}
    }
  else
    _putchar('\n');
}
